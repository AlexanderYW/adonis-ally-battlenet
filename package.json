{
  "name": "adonis-ally-battlenet",
  "version": "0.0.0",
  "description": "Battlenet ally driver for AdonisJS ally",
  "main": "build/providers/index.js",
  "files": [
    "build/adonis-typings",
    "build/instructions.md",
    "build/src",
    "build/providers"
  ],
  "types": "build/providers/index.d.ts",
  "scripts": {
    "clean": "del-cli build",
    "pretest": "npm run lint",
    "test": "node japaFile.js",
    "compile": "npm run lint && npm run clean && tsc && copyfiles instructions.md build",
    "build": "npm run compile",
    "prepublishOnly": "npm run build",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write .",
    "release": "np",
    "version": "npm run build"
  },
  "keywords": [
    "adonisjs",
    "adonisjs-ally",
    "adonis-ally",
    "battlenet"
  ],
  "author": "Alexander Wennerstr√∏m",
  "license": "MIT",
  "devDependencies": {
    "@adonisjs/ally": "^4.1.4",
    "@adonisjs/core": "^5.8.5",
    "@adonisjs/mrm-preset": "^5.0.3",
    "@adonisjs/require-ts": "^2.0.12",
    "@adonisjs/sink": "^5.4.0",
    "@types/node": "^18.6.4",
    "copyfiles": "^2.4.1",
    "del-cli": "^5.0.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-adonis": "^2.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "japa": "^4.0.0",
    "np": "^7.6.2",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "np": {
    "contents": ".",
    "anyBranch": false
  },
  "peerDependencies": {
    "@adonisjs/core": "^5.0.0"
  },
  "adonisjs": {
    "instructionsMd": "./build/instructions.md",
    "env": {
      "BATTLENET_CLIENT_ID": "",
      "BATTLENET_CLIENT_SECRET": ""
    },
    "types": "adonis-ally-battlenet",
    "providers": [
      "adonis-ally-battlenet"
    ]
  },
  "dependencies": {
    "@poppinss/utils": "^5.0.0"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlexanderYW/adonis-ally-battlenet.git"
  },
  "bugs": {
    "url": "https://github.com/AlexanderYW/adonis-ally-battlenet/issues"
  },
  "homepage": "https://github.com/AlexanderYW/adonis-ally-battlenet#readme",
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  }
}
